{
  "expo": {
    "name": "KidsU",
    "description": "ClockIt is a cloud-based time tracking and scheduling app for employees to clcok in and out from their smartphones",
    "icon": "./assets/icon.png",
    "slug": "ClockIt",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    
    "ios": {
      "bundleIdentifier": "com.clockIt.KidsU",
      "supportsTablet": true
    },
    "android": {
      "package": "com.clockIt.KidsU",
      "versionCode":2,
      "permissions": [
        ""
      ]
    },

    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.jpg",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ]
  }
}